  "dependencies": {
    "axios": "^0.26.1",
    "dotenv-cli": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "react": "^17.0.2",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
